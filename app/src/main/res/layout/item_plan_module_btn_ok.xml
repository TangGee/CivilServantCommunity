<?xml version="1.0" encoding="utf-8"?>
<com.mdove.dependent.common.view.roundcorner.RoundCornerConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:layout_marginTop="@dimen/edit_plan_module_margin"
    android:layout_marginLeft="@dimen/edit_plan_module_margin"
    android:layout_marginRight="@dimen/edit_plan_module_margin"
    app:allRadius="12dp">

    <TextView
        android:id="@+id/tv_module_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="12dp"
        android:gravity="center"
        android:text="生成今日计划"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</com.mdove.dependent.common.view.roundcorner.RoundCornerConstraintLayout>

